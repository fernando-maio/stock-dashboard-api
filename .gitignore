# Dependencies
/vendor
/node_modules

# Environment variables
.env
.env.backup
.env.*.local # Catches .env.local, .env.development.local, etc.
.env.production # If you have a specific production .env and manage it outside git, keep it. Otherwise, consider if it's necessary.

# Cache and temporary files
.phpunit.result.cache
/.phpunit.cache # Sometimes generated by PHPUnit itself
/bootstrap/cache/*.php
/storage/*.key
/storage/pail # If you use this specific driver
/storage/logs/*.log
*.log # Catches logs directly in the root

# IDE and OS files
.DS_Store
Thumbs.db
.editorconfig # If you use it and don't want to share
.phpactor.json # PHP Actor configuration
/.fleet/ # JetBrains Fleet IDE
/.idea/ # JetBrains IDEs (PHPStorm, IntelliJ)
/.nova/ # Nova editor
/.vscode/ # VS Code configuration
/.zed/ # Zed editor
Homestead.json
Homestead.yaml
.vagrant/ # Vagrant specific files
auth.json # Composer authentication file (sensitive)

# Public assets (if Laravel Mix/Vite outputs here)
/public/build
/public/hot
/public/storage # Symbolic link, should not be versioned

# Modular setups (if applicable, uncomment if you use Laravel Modules)
# /Modules/*/node_modules
# /Modules/*/vendor
# /Modules/*/composer.lock
# /Modules/*/package-lock.json

# Docker
.dockerignore
docker-compose.override.yml # If you have a local override for Docker Compose